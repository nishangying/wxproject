.phone {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding: 2%;
  background:#f5f2e3;
}
.phone .row{
  width: 100%;
}
.title{
  font-size: 16px;
  font-weight: bold;
  width: 100%;
  margin-bottom: 10rpx;
}
.phone .row-child{
  width: 100%;
  padding: 1rpx 20rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  background:#fff;
  margin: 10px 0;
  box-sizing: border-box;
  border-radius: 10rpx;
}

.phone .row-child .phoneicon{
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.phone .row-child .name{
  width: 200rpx;
  background:#fff;
}
.phone .row-child .phone{
  background:#fff;
}
